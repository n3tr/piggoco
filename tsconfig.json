{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "sourceMap": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts"]
}
